<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net472;net6.0</TargetFrameworks>
    <RootNamespace>NServiceBus.Persistence.RavenDB</RootNamespace>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>..\NServiceBus.snk</AssemblyOriginatorKeyFile>
    <Description>RavenDB persistence support for NServiceBus</Description>
    <LangVersion>10.0</LangVersion>
    <WriteMinVerProperties>true</WriteMinVerProperties>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Fody" Version="6.6.4" PrivateAssets="All" />
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="7.0.0" />
    <PackageReference Include="NServiceBus" Version="[8.0.0, 9)" />
    <PackageReference Include="NuGet.Versioning" Version="6.5.0" />
    <PackageReference Include="Obsolete.Fody" Version="5.3.0" PrivateAssets="All" />
    <PackageReference Include="Particular.Packaging" Version="2.3.0" PrivateAssets="All" />
    <PackageReference Include="RavenDB.Client" Version="[5.4.100, 6.0.0)" />
  </ItemGroup>

</Project>
